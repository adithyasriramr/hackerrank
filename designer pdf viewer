arr=list(map(int,input().split(' ')[:26]))
s=input()
letter=['a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z']
res=[]
for i in range(len(s)):
    for j in range(26):
        if s[i]==letter[j]:
            res.append(arr[j])
print(len(s)*max(res))
