t=int(input())
arr=[]
for i in range(t):
    y=int(input())
    arr.append(y)
for i in range(t):
    count=0
    for j in range(arr[i]+1):
        if arr[i]==0:
            count=1
            break
        elif j%2==0:
            count+=1
        elif j%2!=0:
            count*=2
    print(count)
